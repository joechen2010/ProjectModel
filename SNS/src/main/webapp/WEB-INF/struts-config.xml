		<action path="/admin/report" type="cn.jcenterhome.web.action.admin.ReportAction" scope="request"
			validate="false">
			<forward name="report" path="/admin/report.jsp" />
		</action>
		<action path="/admin/share" type="cn.jcenterhome.web.action.admin.ShareAction" scope="request"
			validate="false">
			<forward name="share" path="/admin/share.jsp" />
