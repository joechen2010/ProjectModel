		<action path="/admin/magic" type="cn.jcenterhome.web.action.admin.MagicAction" scope="request"
			validate="false">
			<forward name="magic" path="/admin/magic.jsp" />
		</action>
		<action path="/admin/magiclog" type="cn.jcenterhome.web.action.admin.MagicLogAction" scope="request"
			validate="false">
			<forward name="magiclog" path="/admin/magiclog.jsp"></forward>
		</action>
		<!-- user -->
		<action path="/admincp" type="cn.jcenterhome.web.action.AdmincpAction" scope="request"
			validate="false" />
		<action path="/cp" type="cn.jcenterhome.web.action.CpAction" scope="request" validate="false" />
		<action path="/do" type="cn.jcenterhome.web.action.DoAction" scope="request" validate="false" />
		<action path="/magic" type="cn.jcenterhome.web.action.MagicAction" scope="request" validate="false" />
