			validate="false">
			<forward name="config" path="/admin/config.jsp" />
		</action>
		<action path="/admin/credit" type="cn.jcenterhome.web.action.admin.CreditAction" scope="request"
			validate="false">
			<forward name="credit" path="/admin/credit.jsp" />
