<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.2//EN" "http://struts.apache.org/dtds/struts-config_1_2.dtd">

<struts-config>
	<data-sources />

	<form-beans>
		<form-bean name="spaceForm" type="org.apache.struts.action.DynaActionForm">
			<form-property name="newvideopic" type="org.apache.struts.upload.FormFile" />
		</form-bean>
		<form-bean name="eventclassForm" type="org.apache.struts.action.DynaActionForm">
			<form-property name="poster" type="org.apache.struts.upload.FormFile" />
		</form-bean>
	</form-beans>

	<global-exceptions />

	<global-forwards>
		<forward name="cpmessage" path="/admin/message.jsp" />
		<forward name="showmessage" path="/template/default/showmessage.jsp" />
	</global-forwards>

	<action-mappings>
		<!-- admin -->
		<action path="/admin/ad" type="cn.jcenterhome.web.action.admin.AdAction" scope="request"
			validate="false">
			<forward name="ad" path="/admin/ad.jsp" />
		</action>
		<action path="/admin/album" type="cn.jcenterhome.web.action.admin.AlbumAction" scope="request"
			validate="false">
			<forward name="album" path="/admin/album.jsp" />
		</action>
		<action path="/admin/app" type="cn.jcenterhome.web.action.admin.AppAction" scope="request"
			validate="false">
			<forward name="app" path="/admin/app.jsp" />
		</action>
		<action path="/admin/backup" type="cn.jcenterhome.web.action.admin.BackupAction" scope="request"
			validate="false">
			<forward name="backup" path="/admin/backup.jsp" />
