			validate="false">
			<forward name="cron" path="/admin/cron.jsp" />
		</action>
		<action path="/admin/doing" type="cn.jcenterhome.web.action.admin.DoingAction" scope="request"
			validate="false">
			<forward name="doing" path="/admin/doing.jsp" />
		</action>
		<action path="/admin/event" type="cn.jcenterhome.web.action.admin.EventAction" scope="request"
			validate="false">
			<forward name="event" path="/admin/event.jsp" />
