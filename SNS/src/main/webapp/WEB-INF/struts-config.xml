		<action path="/admin/click" type="cn.jcenterhome.web.action.admin.ClickAction" scope="request"
			validate="false">
			<forward name="click" path="/admin/click.jsp" />
		</action>
		<action path="/admin/comment" type="cn.jcenterhome.web.action.admin.CommentAction" scope="request"
			validate="false">
			<forward name="comment" path="/admin/comment.jsp" />
